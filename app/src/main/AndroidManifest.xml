<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhizhen.ybb">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name=".base.YbBaseApplication"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".loginpass.WelcomActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".my.MyVison"/>
        <activity
            android:name="com.zhizhen.ybb.loginpass.ForgatPasswordActivity"
            android:screenOrientation="portrait"
             >
        </activity>
        <activity
            android:name="com.zhizhen.ybb.loginpass.LoginActivity"
            android:screenOrientation="portrait"
          >
        </activity>
        <activity
            android:name="com.zhizhen.ybb.loginpass.RegisterActivity"
            android:screenOrientation="portrait"
             >
        </activity>
        <activity
            android:name="com.zhizhen.ybb.my.MyActivity"
            android:screenOrientation="portrait"
            >
        </activity>
    </application>

</manifest>